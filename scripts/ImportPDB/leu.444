#!/usr/bin/perl
$my_db ="a00s_230";
$my_user = "a00s_230";
$my_pass = "testando";
$my_host="127.0.0.1";
$pdb = uc(@ARGV[0]);
chomp $pdb;
unless($pdb){
    print "Use ./script.444 3NIR \n\n";
    exit;
}
print "Limpando\n";
`mysql -h $my_host -u $my_user -p$my_pass $my_db << EOF
UPDATE a_306280 SET i_331770=NULL WHERE i_306337="Leu" AND i_307676='$pdb';
EOF`;

print "-N1\n";
`mysql -h $my_host -u $my_user -p$my_pass $my_db << EOF
